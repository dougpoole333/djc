{
  "name": "bismuth-themekit",
  "version": "1.0.0",
  "description": "Shopify theme boilerplate",
  "private": true,
  "scripts": {
    "deploy": "webpack --config webpack.config.js && theme deploy",
    "start": "npm run deploy && npm run watch",
    "watch": "concurrently \"npm run tab:gulp\" \"npm run tab:theme:watch\"",
    "tab:gulp": "gulp watch",
    "tab:theme:watch": "theme watch --notify=theme_reload"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dougpoole333/bismuth-themekit.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dougpoole333/bismuth-themekit/issues"
  },
  "homepage": "https://github.com/dougpoole333/bismuth-themekit#readme",
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.1.0",
    "event-stream": "^4.0.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-liquidjs": "^1.0.2",
    "gulp-livereload": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-touch": "^1.0.1",
    "gulp-touch-fd": "github:funkedigital/gulp-touch-fd",
    "gulp-uglify": "^3.0.2",
    "gulp-util": "^3.0.8",
    "isomorphic-fetch": "^2.2.1",
    "js-yaml": "^3.13.1",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-stream": "^5.2.1"
  },
  "dependencies": {
    "browser-sync": "^2.26.7",
    "fs": "0.0.1-security"
  }
}
